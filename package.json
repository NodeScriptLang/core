{
    "name": "@nodescript/core",
    "version": "0.0.1-0",
    "type": "module",
    "description": "Visual programming language for Browser and Node",
    "exports": {
        "./types": "./out/main/types/index.js",
        "./util": "./out/main/util/index.js",
        "./schema": "./out/main/schema/index.js",
        "./model": "./out/main/model/index.js",
        "./runtime": "./out/main/runtime/index.js",
        "./nodes": "./out/main/nodes/index.js"
    },
    "files": [
        "out/main/**/*"
    ],
    "scripts": {
        "clean": "rm -rf out *.tsbuildinfo",
        "prepare": "node ./prepare.js",
        "dev": "run-s clean prepare dev:ts",
        "dev:ts": "tsc -b -w",
        "compile": "run-s compile:*",
        "compile:ts": "tsc -b",
        "lint": "eslint --ext=.js,.ts --cache .",
        "test": "NODE_ENV=test mocha --node-option=experimental-network-imports",
        "preversion": "npm run lint",
        "version": "npm run compile",
        "postversion": "npm publish --access=public && git push --tags origin main"
    },
    "engines": {
        "node": ">=18"
    },
    "pre-commit": [
        "lint"
    ],
    "author": "UBIO",
    "dependencies": {
        "airtight": "^4.1.0",
        "nanoid": "^3.3.4",
        "typesafe-event": "^2.0.1"
    },
    "devDependencies": {
        "@types/glob": "^7.2.0",
        "@types/mocha": "^9.1.1",
        "@types/node": "^17.0.35",
        "@ubio/eslint-config": "^1.3.5",
        "esbuild": "^0.14.39",
        "eslint": "^7.32.0",
        "glob": "^8.0.3",
        "http-server": "^14.1.0",
        "mocha": "^10.0.0",
        "npm-run-all": "^4.1.5",
        "ts-node": "^10.8.0",
        "typescript": "^4.6.4",
        "yaml": "^2.1.0"
    }
}
